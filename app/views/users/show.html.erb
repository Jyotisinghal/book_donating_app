<%= provide(:name, "@user.name") %>
<div class="row">
	<aside class="col-md-4">
		<section class="user_info">
			<h1>
  				<%= gravatar_for @user %>
  				<%= @user.name %>
			</h1>
				
		</section>
	</aside>
</div>
<hr> 
<h5>User Email:</h5> <%= @user.email %>, <%= @user.created_at %>,<%= @user.updated_at %><br><h5>Current Time:</h5> <%= Time.now %>



